{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--extensionDevelopmentPath=${workspaceFolder}/../notebook-extension-samples/notebook-renderer",
				"--extensionDevelopmentPath=${workspaceFolder}/../notebook-extension-samples/notebook-provider",
				"${workspaceFolder}/samplenotebooks"
			],
			"env": {
				"CONDA_PREFIX": "/opt/conda"
			},
			"outFiles": [
				"${workspaceFolder}/out/**/*.js",
				"${workspaceFolder}/../notebook-extension-samples/**/*.js"
			],
			"preLaunchTask": "${defaultBuildTask}"
		},
		{
			"name": "Extension Tests",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--extensionTestsPath=${workspaceFolder}/out/test/suite/index"
			],
			"outFiles": [
				"${workspaceFolder}/out/test/**/*.js"
			],
			"preLaunchTask": "${defaultBuildTask}"
		},
		{
			"type": "pwa-node-terminal",
			"name": "Create JavaScript Debug Terminal",
			"request": "launch",
			"cwd": "/Users/weinand/MS/Projects/vscode-simple-jupyter-notebook"
		}
	]
}