{
  "name": "simple-jupyter-notebook",
  "displayName": "simple-jupyter-notebook",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:simple-jupyter-notebook.change-kernel",
    "onCommand:simple-jupyter-notebook.restart-kernel",
    "onDebug:xeus"
  ],
  "enableProposedApi": true,
  "contributes": {
    "debuggers": [
      {
        "type": "xeus",
        "label": "xeus Debug"
      }
    ],
    "commands": [
      {
        "command": "simple-jupyter-notebook.change-kernel",
        "title": "Simple Jupyter Notebook: Change Kernel"
      },
      {
        "command": "simple-jupyter-notebook.restart-kernel",
        "title": "Simple Jupyter Notebook: Restart Kernel"
      },
      {
        "command": "simple-jupyter-notebook.startXeusDebugging",
        "title": "Simple Jupyter Notebook: Start Xeus Debugging"
      }
    ],
    "configuration": [
      {
        "title": "Simple Jupyter Kernels",
        "properties": {
          "simple-jupyter.searchPaths": {
            "description": "List of folders to search to discover Jupyter kernels",
            "default": [],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "updatetypes": "cd src/types && vscode-dts dev && vscode-dts master",
    "test": "npm run test:lint && npm run test:fmt",
    "test:lint": "eslint \"src/**/*.ts\"",
    "test:fmt": "prettier --list-different \"src/**/*.ts\"",
    "fmt": "prettier --write \"src/**/*.ts\"&& npm run test:lint -- --fix"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "avoid",
    "printWidth": 100,
    "tabWidth": 2
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/split2": "^2.1.6",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-plugin-header": "^3.0.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@nteract/messaging": "^7.0.6",
    "get-port": "^5.1.1",
    "rxjs": "^6.5.5",
    "split2": "^3.1.1",
    "zeromq": "^6.0.0-beta.6",
    "vscode-debugprotocol": "1.41.0-pre.0"
  }
}
